info:
    app:
        build-number: '@build.number@'
        description: API endpoints for the Budget Application
        name: Budget App API
        version: '@project.version@'
keycloak:
   auth-server-url: http://localhost:8080/auth
   bearer-only: true
   credentials.secret: 4aaba11f-72ee-4123-9831-ea5f37a7ebb2
   realm: Demo-Realm
   resource: springboot-microservice
   ssl-required: external
   use-resource-role-mappings: true
server:
   context-path: /budget
   http2:
      enabled: true
   port: 8081
   error:
      whitelabel:
         enable: false
   ssl:
      key-store: classpath:majicode.p12
      key-store-password: password$123
      key-store-type: PKCS12
      key-alias: majicode
spring:
   application:
      name: budget-app
   data:
      jpa:
         repositories:
            enabled: true
   datasource:
      driver-class-name: org.h2.Driver
      url: jdbc:h2:mem:test;
      username: sa
      password: sa
   h2:
      console:
         enabled: true
   jackson:
      serialization:
         WRITE_DATES_AS_TIMESTAMPS: false
   jpa:
      hibernate:
         ddl-auto: none
      properties:
         hibernate:
            format_sql: true
      show-sql: false
   liquibase:
      change-log: classpath:/db/changelog/changelog-master.xml
      enabled: true
   mvc:
      log-request-details: true
   thymeleaf:
      enabled: false
springfox:
   documentation:
      swagger:
         v2:
            path: /api-docs
management:
   security:
      enabled: false
---
spring:
   profiles: dev
server:
   port: 8081
---
spring:
   profiles: test
server:
   port: 8082